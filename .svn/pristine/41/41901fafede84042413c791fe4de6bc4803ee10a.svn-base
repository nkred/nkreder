*{
	margin: 0;
	padding: 0;
}
li{
	list-style: none;
}
a
{
	text-decoration: none;
}
img{
	border: none;
}

.clearfix::after
{
	display: block;
	content: '';
	width: 0;
	height: 0;
	clear: both;
}
body{
	width: 100%;
}
.tittle{
	width: 100%;
	height: 4.5rem;
}
.tittl-center{
	width: 93%;
	margin: 0 auto;
	padding-top: 1.1rem;
}
.tittl-center>img{
	width: 10rem;
	height: 2.3rem;
	float: left;
}
.tittle-right{
	float: right;
	width: 13.5rem;
	height: 2.3rem;
}
.tittle-right-left{
	width: 10.9rem;
	height: 2.2rem;
	border: 0.05rem #ccc solid;
	border-radius:1rem ;
	position: relative;
	float: left;
}
.tittle-right-left>input{
    width: 9rem;
    height: 2.2rem;
    margin-left: 0.6rem;
	outline: none;
	-webkit-appearance: none;
	border-radius: 0;
	border:0;
	text-indent:10px;
	font-size: 1.1rem;
	
}
.tittle-right-left>input::-ms-input-placeholder{
text-align: center;
}
.tittle-right-left>input::input-placeholder{
text-align: center;
}
.tittle-right-left>input::-webkit-input-placeholder{
text-align: center;
}
.tittle-right-left>a{
     position: absolute;
     top: 0.35rem;
     right: 0.5rem;
}
.tittle-right-left>a img{
     width: 1.65rem;
     height: 1.4rem;
}
.tittle-right>a{
     margin-top: 0.2rem;
     width: 2.25rem;
     height: 2rem;
     float: right;
}
.tittle-right>a img{
     width: 2.25rem;
     height: 2rem;
}



#banner{
    width: 100%;
    position: relative;
    overflow: hidden;
}
#banner-all{
    width: 100%;
    transition: all 1.5s;
    position: relative;
    left: 0;
    top: 0;
}
#banner-all li{
    float: left;
}
#banner-all li a img{
    width: 100%;
    height: 10rem;
}


.nav{
    width: 100%;
    height: 13.8rem;
    background: #f9f9f9;
    padding: 1.2rem 0;
    margin-top: -0.3rem;
}
.nav-flex1{
	width: 100%;
    display: flex;
    justify-content: space-around;
    align-items: center;
}

.nav-flex1 li a{
    display: flex;
    flex-direction: column;
    justify-content: space-around;
    align-items: center;
    color: #9b9b9b;
}
.nav-flex1 li img{
     width: 4.25rem;
     height: 4.25rem;
}
.nav-flex2{
    margin-top: 1.4rem;
	width: 100%;
    display: flex;
    justify-content: space-around;
    align-items: center;
}

.nav-flex2 li a{
    display: flex;
    flex-direction: column;
    justify-content: space-around;
    align-items: center;
    color: #9b9b9b;
}
.nav-flex2 li img{
     width: 4.25rem;
     height: 4.25rem;
}


.hd{
     width:100% ;
     height: 5.9rem;
     background: #efefef;
}
.hd img{
     width: 97%;
     height: 4rem;
     margin-top: 0.9rem;
     margin-left: 0.5rem;
}

.inner{
	width: 100%;
	position: relative;
	display: flex;
    justify-content: space-around;
    align-items: center;
    flex-wrap: wrap;
    background: #efefef;
}
.inner>img{
	width: 6.1rem;
	height: 1.9rem;
	position: absolute;
	top: 0;
	left: 1.2rem;
}
.inner>div{
	display: flex;
    flex-direction: column;
    justify-content: space-around;
    align-items: center;
}
.inner>div>img{
	width: 9rem;
	height: 6.5rem;
}
.inner>div>div{
	background: #fff;
	width: 8rem;
	height: 5.85rem;
	padding: 0 0.5rem;
}
.inner>div>div>p{
	font-size: 1.1rem;
	
}
.inner>div>div>div{
	
	margin-top:1rem ;
}
.inner>div>div>div p{
	float: left;
	color: red;
	margin-right: 0.5rem;
	font-size: 0.5rem;
}
.inner>div>div>div p:last-child{
	font-size:0.4rem ;
	color: #b5b5b5;
	margin-right: 0;
	float: right;
}

.iner{
	width: 100%;
	background: #efefef;
}
.iner-one{
	width: 100%;
	height: 6rem;
}
.iner-one img{
	width: 97%;
	height: 4rem;
	margin-left: 0.5rem;
	margin-top: 1rem;
}

.iner-two{
	width: 100%;
}
.iner-two ul{
	display: flex;
	justify-content: space-around;
	align-items: center;
    
}
.iner-two ul li{
	background: #fff;
	margin-bottom: 0.8rem;
}
.iner-two ul li>img{
	width: 17.25rem;
}
.iner-two ul li>div{
	width: 15.25rem;
	font-size: 1.25rem;
	padding: 0 1rem;
}
.iner-two ul li>div div{
	margin-top: 2rem;
	margin-bottom: 2rem;
}
.iner-two ul li>div div p{
	float: left;
	font-size: 0.8rem;
	color: #ff5200;
}
.iner-two ul li>div div p:first-child span{
	font-size: 1.2rem;
}
.iner-two ul li>div div p:last-child{
	color: #ccc;
	margin-top:0.5rem;
	float: right;
}


.foot{
	width: 100%;
	background: #fcfcfc;
	padding: 0.75rem 0;
}
.foot ul{
	display: flex;
	justify-content: space-around;
	align-content: center;
}
.foot ul li img{
	width: 2.1rem;
	height: 2.15rem;
	margin-bottom: 0.5rem;
}
.foot ul li a p{
	color: #7b7b7b;
}
