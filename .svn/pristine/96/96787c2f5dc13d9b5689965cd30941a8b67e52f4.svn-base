.inner-right>div>p{
	
	font-size: 1.2rem;
	color: #585858;
	font-weight: bold;
	margin-bottom: 0.75rem;
	margin-top: 1rem;
}
.inner-right-two{
	width: 22.5rem;
	height: 18.8rem;
	padding: 1.8rem;
	display: flex;
	justify-content: space-between;
	flex-wrap: wrap;
	background: #fff;
}
.inner-right-two a img{
	width:6.15rem;
	height:6.15rem; 	
}
.inner-right-two a{
	display: block;
	display: flex;
	justify-content: space-around;
	flex-direction: column;
	align-items:center ;
	color: #585858;
	font-size: 1.2rem;
	font-weight: bold;
}
.inner-right-two div{
	display: flex;
	justify-content: space-around;
	flex-direction: column;
	align-items:center ;
}
.inner-left-kong{
	width: 22.5rem;
	height: 5rem;
}




.foot{
	width: 100%;
	height: 5rem;
	position: fixed;
	bottom: 0;
	left: 0;
	background: #fbfbfb;
	display: flex;
	justify-content: space-around; 
	align-items:center ;
}
.foot a{
	color: #7b7b7b;
	display: block;
	display: flex;
	justify-content: space-around;
	flex-direction: column;
	align-items:center ;
}
.foot a img{
	width: 2.2rem;
	height: 2.1rem;
}
